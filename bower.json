{
  "name": "PdfRenderer",
  "version": "0.0.3",
  "homepage": "https://github.com/PEM--/pdfrenderer",
  "authors": [
    "Pierre-Eric Marchandet <pemarchandet@gmail.com>"
  ],
  "description": "Client side PDF renderer",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bower-pdfkit": "0.7.0",
    "FileSaver": "*"
  },
  "ignoredDependencies": [
    "jquery"
  ],
  "overrides": {
    "bower-pdfkit": { "arch": [ "client" ] },
    "FileSaver": {
      "main": [ "FileSaver.js"],
      "arch": [ "client" ]
    }
  }
}
